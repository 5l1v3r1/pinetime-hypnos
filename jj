-	int err;
-
-	err = adc_monitor_init();
-	if (err < 0) {
-		return err;
-	}
-
-	err = charge_pins_init();
-	if (err < 0) {
-		return err;
-	}
-
-	callback = cb;
-
-	return 0;
-}
