-	SHELL_CMD_ARG(status, NULL,
-			"Status", cmd_battery_status, 1, 0),
-	SHELL_SUBCMD_SET_END
-);
-
-SHELL_CMD_REGISTER(battery, &sub_battery, "Commands for battery monitor",
-		   NULL);
